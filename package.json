{
  "name": "prostep-js",
  "version": "1.5.3",
  "description": "A JS Task Library",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./filesystemDeleteTask": "./dist/lib/tasks/filesystemDeleteTask.js",
    "./httpDownloadFileTask": "./dist/lib/tasks/httpDownloadFileTask.js",
    "./httpUploadFileTask": "./dist/lib/tasks/httpUploadFileTask.js"
  },
  "typesVersions": {
    "*": {
      "index.d.ts": [
        "dist/index.d.ts"
      ]
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsc --project .",
    "test": "npx mocha",
    "lint": "eslint .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@types/chai": "^4.3.4",
    "@types/js-yaml": "^4.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.3",
    "@types/npmlog": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "chai": "^5.1.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.4.0",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "dependencies": {
    "alpha8-lib": "link:../alpha8-lib",
    "axios": "^1.6.8",
    "execa": "^8.0.1",
    "js-yaml": "^4.1.0",
    "npmlog": "^7.0.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ]
  },
  "alpha8Config": {
    "eventDriver": "local"
  }
}
